    <span class="star <%= userName.replace(/\./g,'_').replace('@','_') %>" style="margin: -1%; scale: 0.5"></span>
    <span class="star <%= userName.replace(/\./g,'_').replace('@','_') %>" style="margin: -1%; scale: 0.5"></span>
    <span class="star <%= userName.replace(/\./g,'_').replace('@','_') %>" style="margin: -1%; scale: 0.5"></span>
  </div>
  <p><%= message %></p>
  <form method="post" action="/delrev" ><input name="stars" value="stars_given"><input name="procode" value="<%= proCode %>"><button type="submit" class="<%= deleteButton %>"><span></span></button></form>
  <hr style="border-top: dotted 4px; width: 25%; margin-left: 37.5%; margin-top: 1.5%; margin-bottom: 3%; background-color: white;">
</div>

<script>
  var stars = document.querySelectorAll( ".star.<%=userName.replace(/\./g,'_').replace('@','_') %>");
    for (let i = 0; i < 5; i++) {
                  if ( i <= <%=  stars_given -1 %> ){
                      stars[i].classList.add("selected")
                  }
                  else {
     stars[i].classList.remove("selected")};
         }
</script>