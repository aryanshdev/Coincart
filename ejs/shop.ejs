<%- include("header.ejs") -%>


  <%- include("navbar.ejs") -%>
  <main>

      <div class="hero-area section-bg2">
        <div class="container">
          <div class="row">
            <div class="col-xl-12">
              <div class="slider-area">
                <div
                  class="slider-height2 slider-bg4 d-flex align-items-center justify-content-center"
                >
                  <div class="hero-caption hero-caption2">
                    <h2> <%= pageName %></h2>
                    <nav aria-label="breadcrumb">
                      <ol class="breadcrumb justify-content-center">
                        <li class="breadcrumb-item">
                          <a href="/">Home</a>
                        </li>
                        <li class="breadcrumb-item">
                          <a href="#">Category</a>
                        </li>
                      </ol>
                    </nav>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="listing-area pt-50 pb-50">
        <div class="container">
          <div class="row">

            <div class="col-xl-12">
              <div class="latest-items latest-items2">
                <div class="row">
                    <% for (let i = 0; i < products.length; i++) { %>
                      <%- include("product-listing.ejs", {productImg :products[i].product_img ,productCode: products[i].id, productName:products[i].product_name, productPrice:products[i].product_price, productMRP:products[i].product_mrp }) -%>
                    <% } %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>

    <%- include("footer.ejs") %>

    <div id="back-top">
      <a class="wrapper" title="Go to Top" href="#">
        <div class="arrows-container">
          <div class="arrow arrow-one"></div>
          <div class="arrow arrow-two"></div>
        </div>
      </a>
    </div>

    <script src="./assets/js/vendor/modernizr-3.5.0.min.js"></script>
    <script src="./assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="./assets/js/popper.min.js"></script>
